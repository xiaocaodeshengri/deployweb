                     });\r\n                        } else {\r\n                            this.token = response.data['res']['token'];\r\n                            if (this.token) {\r\n                                this.$store.commit(types.LOGIN, this.token);\r\n                                this.$store.commit(types.ROLE, response.data['res']['role']);\r\n                                this.$store.commit('role', response.data['res']['role']);\r\n                                this.$store.commit(types.USERNAME, response.data['res']['username']);\r\n                                this.$store.commit('username', response.data['res']['username']);\r\n                                this.$store.commit(types.NAME, response.data['res']['name']);\r\n                                this.$store.commit('name', response.data['res']['name']);\r\n                                this.$store.commit(types.CATEGORY, response.data['res']['category']);\r\n                                this.$store.commit('category', response.data['res']['category']);\r\n                                this.$store.commit(types.CATEGORY_ID, response.data['res']['category_id']);\r\n                                this.$store.commit('category_id', response.data['res']['category_id']);\r\n                                let redirect = decodeURIComponent(this.$route.query.redirect || '/');\r\n                                this.$router.push({path: redirect})\r\n                            }\r\n                        }\r\n                    }\r\n                );\r\n            }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import '@/assets/css/color.scss';\r\n\r\n    .page-login {\r\n        @extend %unable-select;\r\n        $backgroundColor: #F0F2F5;\r\n        // ---\r\n        background-color: $backgroundColor;\r\n        height: 100%;\r\n        width: 100%;\r\n        position: absolute;\r\n        // 层\r\n        .page-login--layer {\r\n            @extend %full;\r\n   